    "gatsby-source-pg": "^0.1.4",
    "gatsby-transformer-sharp": "^2.10.0",
    "postcss": "^8.2.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0"
