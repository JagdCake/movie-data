    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test-db": "psql -d test_movies -f database-functions.test.sql",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx",
